{
   "name": "unifarm-indexing-all",
   "version": "1.0.0",
   "main": "index.ts",
   "license": "MIT",
   "scripts": {
      "cohort-seed": "export NODE_ENV=dev && ts-node scripts/cohort.ts",
      "token-seed": "export NODE_ENV=dev && ts-node scripts/tokens.ts",
      "ethereum-stake-event-seed": "export NODE_ENV=dev && ts-node events/db-seed/db-seed-stake-ethereum.ts",
      "ethereum-unstake-event-seed": "export NODE_ENV=dev && ts-node events/db-seed/db-seed-unstake-ethereum.ts",
      "ethereum-claim-event-seed": "export NODE_ENV=dev && ts-node events/db-seed/db-seed-claim-ethereum.ts",
      "ethereum-refferal-claim-event-seed": "export NODE_ENV=dev && ts-node events/db-seed/db-seed-refferal-claim-ethereum.ts",
      "ethereum-unstake-event-seed-proxy": "export NODE_ENV=dev && export PROXY=yes && ts-node events/db-seed/db-seed-unstake-ethereum.ts",
      "ethereum-claim-event-seed-proxy": "export NODE_ENV=dev && export PROXY=yes && ts-node events/db-seed/db-seed-claim-ethereum.ts",
      "ethereum-refferal-claim-event-seed-proxy": "export NODE_ENV=dev && export PROXY=yes && ts-node events/db-seed/db-seed-refferal-claim-ethereum.ts",
      "create-rewardCap": "export NODE_ENV=dev && ts-node scripts/createRewardCapJson.ts",
      "start:graph-server": "export NODE_ENV=dev && nodemon --ext '.ts' ./graphql/graph.ts",
      "start:eth-listeners": "export NODE_ENV=dev && export CHAIN_ID=1 && nodemon --ext '.ts' events/listeners/initListeners.ts",
      "start:cron-server": "export NODE_ENV=dev && nodemon --ext '.ts' cron/jobs.ts",
      "flush:all-offsets": "export NODE_ENV=dev && nodemon --ext '.ts' cron/flushAll.ts"
   },
   "dependencies": {
      "@ethersproject/abi": "^5.4.1",
      "@ethersproject/address": "^5.4.0",
      "@ethersproject/contracts": "^5.4.1",
      "@ethersproject/providers": "^5.4.5",
      "@ethersproject/units": "^5.4.0",
      "@types/cron": "^1.7.3",
      "@types/node-fetch": "^3.0.3",
      "apollo-server": "^3.3.0",
      "apollo-server-core": "^3.3.0",
      "apollo-server-express": "^3.3.0",
      "bluebird": "^3.7.2",
      "colors": "^1.4.0",
      "commander": "^8.2.0",
      "cron": "^1.8.2",
      "dotenv": "^10.0.0",
      "ethereum-events": "^0.1.3",
      "ethers": "^5.4.7",
      "express": "^4.17.1",
      "graphql": "^15.6.0",
      "lodash": "^4.17.21",
      "mongoose": "^6.0.7",
      "node-fetch": "^2.6.1",
      "pg": "^8.7.1",
      "redis": "^3.1.2",
      "reflect-metadata": "^0.1.13",
      "typeorm": "^0.2.37",
      "web3": "^1.5.3",
      "winston": "^3.3.3",
      "yaml": "^1.10.2"
   },
   "devDependencies": {
      "@types/bluebird": "^3.5.36",
      "@types/lodash": "^4.14.173",
      "@types/node": "^16.9.6",
      "@types/redis": "^2.8.32",
      "@types/yaml": "^1.9.7",
      "@typescript-eslint/parser": "^4.31.2",
      "eslint": "^7.32.0",
      "nodemon": "^2.0.12",
      "prettier": "^2.4.1",
      "ts-node": "^10.2.1",
      "typescript": "^4.4.3"
   }
}
