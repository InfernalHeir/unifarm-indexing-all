{
   "name": "unifarm-indexing-all",
   "version": "1.0.0",
   "main": "index.ts",
   "license": "MIT",
   "scripts": {
      "create-rewards": "ts-node scripts/create-reward-cap.ts",
      "start:seeders": "ts-node ./index.ts",
      "start:dev:graph-server": "export NODE_ENV=dev && nodemon --ext '.ts' ./graphql/graph.ts",
      "start:dev:eth:listeners": "export NODE_ENV=dev && export CHAIN_ID=1 && nodemon --ext '.ts' events/listeners/initListeners.ts",
      "start:dev:bsc:listeners": "export NODE_ENV=dev && export CHAIN_ID=56 && nodemon --ext '.ts' events/listeners/initListeners.ts",
      "start:dev:polygon:listeners": "export NODE_ENV=dev && export CHAIN_ID=56 && nodemon --ext '.ts' events/listeners/initListeners.ts",
      "clean:graph": "rimraf ./graphql/build",
      "build:graph": "babel ./graphql/.babelrc --extensions '.ts' --out-dir ./graphql/build --config-file ./.babelrc --no-copy-ignored --copy-files"
   },
   "dependencies": {
      "@babel/cli": "^7.15.7",
      "@ethersproject/abi": "^5.4.1",
      "@ethersproject/address": "^5.4.0",
      "@ethersproject/contracts": "^5.4.1",
      "@ethersproject/providers": "^5.4.5",
      "@ethersproject/units": "^5.4.0",
      "@types/cron": "^1.7.3",
      "@types/node-fetch": "^3.0.3",
      "apollo-server": "^3.3.0",
      "apollo-server-core": "^3.3.0",
      "apollo-server-express": "^3.3.0",
      "babel-polyfill": "^6.26.0",
      "bluebird": "^3.7.2",
      "colors": "^1.4.0",
      "commander": "^8.2.0",
      "cron": "^1.8.2",
      "dotenv": "^10.0.0",
      "ethereum-events": "^0.1.3",
      "ethers": "^5.4.7",
      "express": "^4.17.1",
      "graphql": "^15.6.0",
      "lodash": "^4.17.21",
      "mongoose": "^6.0.7",
      "node-fetch": "^2.6.1",
      "npm-run-all": "^4.1.5",
      "pg": "^8.7.1",
      "redis": "^3.1.2",
      "reflect-metadata": "^0.1.13",
      "typeorm": "^0.2.37",
      "web3": "^1.5.3",
      "winston": "^3.3.3",
      "yaml": "^1.10.2"
   },
   "devDependencies": {
      "@babel/core": "^7.15.5",
      "@babel/plugin-transform-modules-commonjs": "^7.15.4",
      "@babel/plugin-transform-runtime": "^7.15.0",
      "@babel/preset-env": "^7.15.6",
      "@babel/preset-typescript": "^7.15.0",
      "@babel/runtime": "^7.15.4",
      "@types/bluebird": "^3.5.36",
      "@types/lodash": "^4.14.173",
      "@types/node": "^16.9.6",
      "@types/redis": "^2.8.32",
      "@types/yaml": "^1.9.7",
      "@typescript-eslint/parser": "^4.31.2",
      "babel-plugin-transform-runtime": "^6.23.0",
      "eslint": "^7.32.0",
      "nodemon": "^2.0.12",
      "prettier": "^2.4.1",
      "ts-node": "^10.2.1",
      "typescript": "^4.4.3"
   }
}
